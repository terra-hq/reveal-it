{
  "name": "@terrahq/reveal-it",
  "private": false,
  "version": "1.0.8",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "browser": "./dist/reveal-it.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/reveal-it.umd.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],  
  "scripts": {
    "dev": "vite",
