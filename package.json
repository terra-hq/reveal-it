    "preview": "vite preview",
    "build:umd": "rollup -c rollup.config.umd.js"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/node": "^20.8.5",
    "rollup": "^2.60.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.69.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "@andresclua/jsutil": "^3.0.16",
    "@teamthunderfoot/breakpoints": "^0.0.83",
    "gsap": "^3.12.5"
  },
  "peerDependencies": {
    "gsap": "^3.12.5"
  }
}
